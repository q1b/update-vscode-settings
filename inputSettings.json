{
	// https://play.tailwindcss.com/KRdy96E2yN theme
	// https://play.tailwindcss.com/TRKarp06l4 dark theme
	"$schema": "vscode://schemas/color-theme",
	"vars":{
		"$base":{
			"default":"#fff"
		}
	},
	"scope":["colors"],
	"colors" : {
		"button.background": "565/600-mix->$blue.650&$prime.600",
		"button.foreground": "565/600-mix->$blue.650&$prime.150",
		"button.hoverBackground": "565/600-mix->$blue.650&$prime.550",
		"button.border": "565/600-mix->$blue.650&$prime.300",
		"button.secondaryBackground": "565/600-mix->$blue.650&$base.650",
		"button.secondaryForeground": "565/600-mix->$blue.650&$base.150",
		"button.secondaryHoverBackground": "565/600-mix->$blue.650&$base.600",
		"button.separator": "565/600-mix->$blue.650&$base.400",
		"breadcrumb.background": "565/600-mix->$blue.650&$base.900",
		"breadcrumb.foreground": "565/600-mix->$blue.650&$base.450",
		"breadcrumbPicker.background": "565/600-mix->$blue.650&$base.750",
		"breadcrumb.focusForeground": "565/600-mix->$blue.650&$base.350",
		"activityBar.background": "565/600-mix->$blue.650&$base.1000",
		"activityBar.activeBackground": "565/600-mix->$blue.650&$base.800",
		"activityBar.foreground": "565/600-mix->$blue.650&$base.450",
		"activityBar.inactiveForeground": "565/600-mix->$blue.650&$base.600",
		"activityBar.activeFocusBorder": "565/600-mix->$blue.650&$base.400",
		"activityBar.activeBorder": "565/600-mix->$blue.650&$base.500",
		"activityBar.border": "565/600-mix->$blue.650&$base.700",
		"activityBar.dropBorder": "565/600-mix->$blue.650&$base.800",
		"sideBar.background": "565/600-mix->$blue.650&$base.950",
		"sideBar.foreground": "565/600-mix->$blue.650&$base.350",
		"sideBar.border": "565/600-mix->$blue.650&$base.800",
		"sideBar.dropBackground": "565/600-mix->$blue.650&$base.750/20",
		"sideBarTitle.foreground": "565/600-mix->$blue.650&$base.100",
		"sideBarSectionHeader.background": "565/600-mix->$blue.650&$base.850",
		"sideBarSectionHeader.foreground": "565/600-mix->$blue.650&$base.200",
		"sideBarSectionHeader.border": "565/600-mix->$blue.650&$base.700"
	}
}

{
  /**
   * Better Defaults
   **/
  "zenMode.centerLayout": false,
  "screencastMode.keyboardOverlayTimeout": 500,
  "screencastMode.onlyKeyboardShortcuts": false,
  "screencastMode.mouseIndicatorSize": 20,
  "screencastMode.mouseIndicatorColor": "#FF003310",
  "screencastMode.fontSize": 20,
  "screencastMode.verticalOffset": 10,
  // "screencastMode.mouseIndicatorColor": "#FFF",
  // "screencastMode.keyboardShortcutsFormat": "commandWithGroupAndKeys",
  // "screencastMode.verticalOffset": 10,
  // "screencastMode.onlyKeyboardShortcuts": true,
  // /**
  // * TailwindCSS Settings
  // */
  // "tailwindCSS.lint.recommendedVariantOrder": "warning",
  // "tailwindCSS.lint.invalidTailwindDirective": "warning",
  "explorer.openEditors.visible": 1,
  "editor.bracketPairColorization.enabled": true,
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": false,
  "workbench.editor.enablePreview": false,
  "window.newWindowDimensions": "inherit",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggest.showSnippets": true,
  "editor.suggest.showUsers": true,
  "editor.suggestSelection": "first",
  "editor.detectIndentation": true,
  "window.nativeFullScreen": false,
  "files.insertFinalNewline": true,
  "workbench.editor.showTabs": false,
  "editor.lineNumbers": "off",
  /**
   * Silence The Noise
   */
  "breadcrumbs.enabled": true,
  "scm.diffDecorations": "none",
  "editor.hover.enabled": true,
  "editor.glyphMargin": false,
  // "editor.matchBrackets": "never",
  "editor.matchBrackets": "always",
  "workbench.tips.enabled": false,
  "editor.colorDecorators": true,
  "git.decorations.enabled": false,
  "workbench.startupEditor": "none",
  "editor.lightbulb.enabled": false,
  "editor.selectionHighlight": true,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.occurrencesHighlight": false,
  "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  // "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  /**
   * Fonts
  */
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.4,
  "editor.letterSpacing": 0.2,
  "editor.renderWhitespace": "none",
  "editor.suggestLineHeight": 28,
  "workbench.layoutControl.enabled": true,
  /**
   * Find
   **/
  "search.useIgnoreFiles": false,
  "search.exclude": {
    // Hide everything in /vendor, except the "laravel" and "livewire" folder.
    "**/pnpm-lock.yaml": true,
    "**/vendor/{[^l],?[^ai]}*": true,
    // Hide everything in /public, except "index.php"
    "**/public/{[^i],?[^n]}*": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/_ide_helper.php": true,
    "**/composer.lock": true,
    "**/package-lock.json": true,
    "storage": true,
    ".phpunit.result.cache": true
  },
  /**
   * Code
   **/
  // Include "-" in word selection.
  // "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.includeLanguages": {
    "svelte": "html",
    "vue": "vue",
    "astro": "html"
  },
  // VIM SETTINGS OVERWRITE
  "vim.useSystemClipboard": true,
  // VIM KEYBINDING SETTINGS
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "backspace",
        "backspace"
      ],
      "commands": [
        "editor.emmet.action.removeTag"
      ]
    },
    {
      "before": [
        "o",
        "o"
      ],
      "commands": [
        "editor.emmet.action.balanceOut"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "n",
        "n"
      ],
      "commands": [
        "editor.emmet.action.evaluateMathExpression"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  /**
   * Window Settings
   **/
  // "window.nativeTabs": true,
  // "window.titleBarStyle": "native",
  "editor.cursorBlinking": "phase",
  "amVim.vimStyleNavigationInListView": false,
  "amVim.startInInsertMode": false,
  "cSpell.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "rust": false,
    "cpp": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "astro": "html",
    "svelte": "html"
  },
  "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "clx"
  ],
  //   "window.zoomLevel": 1
  "editor.cursorSurroundingLines": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${appName}",
  "git.autofetch": true,
  "workbench.editor.labelFormat": "short",
  "editor.stickyTabStops": true,
  "prettier.useEditorConfig": true,
  "prettier.trailingComma": "all",
  "prettier.useTabs": true,
  "prettier.tabWidth": 4,
  "window.menuBarVisibility": "compact",
  "editor.guides.indentation": false,
  "editor.tabSize": 2,

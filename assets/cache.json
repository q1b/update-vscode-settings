{
	"json.validate.enable": false,
	"editor.accessibilitySupport": "off",
	"editor.find.addExtraSpaceOnTop": false,
	"editor.guides.bracketPairs": "active",
	// "editor.lineNumbers": "interval",
	// "editor.multiCursorModifier": "ctrlCmd",
	// "editor.renderWhitespace": "boundary",
	"editor.suggestSelection": "first",
	"window.commandCenter": true,
	"editor.inlineSuggest.enabled": true,

	/**
	 * Better Defaults
	 **/
	// Give it to me as fast as you have it.
	"editor.quickSuggestionsDelay": 0,
	"editor.copyWithSyntaxHighlighting": false,
	"diffEditor.ignoreTrimWhitespace": false,
	// When I accidentally hit copy without selecting anything and it overrides my clipboard - ugh.
	"editor.emptySelectionClipboard": false,
	"window.newWindowDimensions": "inherit",
	"editor.snippetSuggestions": "inline",
	"editor.detectIndentation": false,
	"editor.guides.indentation": false,
	"window.dialogStyle": "custom",
	"files.insertFinalNewline": true,
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"workbench.sideBar.location": "left",
	"workbench.editor.showTabs": false,
	"editor.wrappingIndent": "deepIndent",
	"editor.tabSize": 4,

	/**
	 * Silence The Noise
	 */
	"breadcrumbs.enabled": true,
	"breadcrumbs.filePath": "last",
	"scm.diffDecorations": "none",
	"editor.hover.enabled": true,
	"editor.matchBrackets": "never",
	"workbench.tips.enabled": true,
	"git.decorations.enabled": false,
	"workbench.startupEditor": "none",
	"editor.lightbulb.enabled": true,
	"editor.selectionHighlight": true,
	"editor.overviewRulerBorder": false,
	"editor.renderLineHighlight": "none",
	"editor.occurrencesHighlight": false,
	"problems.decorations.enabled": false,
	"editor.renderControlCharacters": false,
	"editor.hideCursorInOverviewRuler": true,
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleDeclarations": "goto",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.list.smoothScrolling": true,
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"editor.wrappingStrategy": "advanced",
	// "editor.wordWrapColumn": 200,

	"editor.glyphMargin": false,
	"editor.autoIndent": "advanced",
	"editor.cursorSmoothCaretAnimation": true,
	"editor.cursorSurroundingLines": 1,
	"editor.cursorSurroundingLinesStyle": "all",
	"editor.scrollbar.verticalScrollbarSize": 8,
	"workbench.enableExperiments": true,
	"workbench.list.fastScrollSensitivity": 5,
	"workbench.tree.expandMode": "singleClick",
	"workbench.tree.indent": 12,

	"editor.lineNumbers": "off",
	"editor.renderWhitespace": "none",

	/**
	 * Typography
	 **/
	"editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
	"editor.tokenColorCustomizations": {
		"comments": { "fontStyle": "bold italic", "foreground": "#6E7A99" }
	},
	"editor.foldingStrategy": "indentation",
	"editor.fontWeight": "400",
	"editor.fontLigatures": "'calt', 'ss01'",
	"tailwindCSS.classAttributes": ["class", "className", "classList"],
	"tailwindCSS.colorDecorators": true,
	"editor.fontSize": 14,
	"editor.lineHeight": 1.4,
	"editor.suggestFontSize": 15,
	"editor.suggestLineHeight": 28,
	"terminal.integrated.fontSize": 15,
	"terminal.integrated.lineHeight": 1.5,
	// "terminal.integrated.fontFamily": "",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "block",
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.persistentSessionReviveProcess": "onExit",
	"terminal.integrated.tabs.enabled": true,
	"terminal.integrated.fontFamily": "'FiraCode NF',monospace",
	"markdown.experimental.validate.fileLinks.markdownFragmentLinks": "warning",
	"editor.minimap.renderCharacters": false,
	"editor.autoClosingBrackets": "always",
	/**
	 * Find
	 **/
	"search.useIgnoreFiles": false,
	"tailwindCSS.files.exclude": [
		"**/.git/**",
		"**/node_modules/**",
		"**/.hg/**",
		"**/package-lock.json",
		"**/pnpm-lock.yaml",
		"**/tmp",
		"**/yarn.lock"
	],
	"files.watcherExclude": {
		"**/.git": true,
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true,
		"**/.hg/store/**": true
	},
	"search.exclude": {
		"**/.git": true,
		"**/.github": true,
		"**/.nuxt": true,
		"**/.output": true,
		"**/.pnpm": true,
		"**/.vscode": true,
		"**/.yarn": true,
		"**/bower_components": true,
		"**/dist/**": true,
		"**/logs": true,
		"**/node_modules": true,
		"**/out/**": true,
		"**/package-lock.json": true,
		"**/pnpm-lock.yaml": true,
		"**/tmp": true,
		"**/yarn.lock": true
	},
	/**
	 * Code
	 **/
	// Include "-" in word selection.
	"editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",

	"window.titleBarStyle": "custom",
	"window.title": "üóÇÔ∏è ${rootName}${separator}${dirty}üìÇ ${activeEditorShort}${separator}",
	"window.titleSeparator": " - ",
	"window.experimental.windowControlsOverlay.enabled": true,
	"editor.experimental.pasteActions.enabled": true,

	/**
	 * VsCode
	 **/
	"vim.surround": true,
	"vim.easymotion": true,
	"vim.easymotionMarkerForegroundColorOneChar": "#ff0000",

	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "k"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		},
		{
			"before": ["<C-n>"],
			"commands": [":nohl"]
		},
		{
			"before": ["K"],
			"commands": ["lineBreakInsert"],
			"silent": true
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["p"],
			"after": ["p", "g", "v", "y"]
		}
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false
	},
	"vim.digraphs": {
		"R!": ["üöÄ", ["0xD83D", "0xDE80"]]
	},
	"vim.autoindent": true,
